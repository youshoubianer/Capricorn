{
  "manifest_version": 2,

  "name": "bookmarksExport",
  "description": "This extension export the bookmarks to json",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "<all_urls>", 
    "bookmarks",
    "tabs",
    "notifications"
  ]
}