{
  "manifest_version": 2,

  "name": "urlCode ",
  "description": "This extension generate qrcode",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "<all_urls>", 
    "tabs"
  ]
}