{
  "manifest_version": 2,

  "name": "uploadClipboardImg ",
  "description": "This extension upload the clipboard image to qiniu",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}